	<Project>
		<Option title="pipemania" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/pipemania" prefix_auto="1" extension_auto="1" />
