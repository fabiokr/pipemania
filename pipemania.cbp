				<Option output="pipemania" prefix_auto="1" extension_auto="1" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add directory="C:/SDL/include" />
					<Add directory="include" />
				</Compiler>
				<Linker>
					<Add option="-lmingw32 -lSDLmain -lSDL" />
					<Add directory="C:/SDL/lib" />
					<Add directory="C:/SDL/bin" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
